<% @memories.each do |memorie| %>
  <div>
    <%= link_to memorie.title, memory_path(memorie) %>

    <% if memorie.images.attached? %>
    <% memorie.images.each do |image| %>

      <%= cl_image_tag image.key, height: 300, width: 400 %>
 <% end %>
    <% else %>
      <%= image_tag "Lune.jpg", height: 300, width: 400 %>
    <% end %>

  </div>
<% end %>
